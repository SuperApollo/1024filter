<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.clfilter.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_errorPageFragment"
            app:destination="@id/errorPageFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_WebFragment"
            app:destination="@+id/WebFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.clfilter.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_WebFragment"
            app:destination="@+id/WebFragment" />
    </fragment>
    <fragment
        android:id="@+id/WebFragment"
        android:name="com.example.clfilter.WebFragment"
        android:label="@string/web_fragment_label"
        tools:layout="@layout/fragment_web" />
    <fragment
        android:id="@+id/errorPageFragment"
        android:name="com.example.clfilter.ErrorPageFragment"
        android:label="ErrorPageFragment" />
</navigation>